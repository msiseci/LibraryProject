@using Library.Models.Entity
@model List<Announcements>

 @foreach (var x in Model)
 {



<div class="post">

    <div class="user-block">

        <img class="img-circle img-bordered-sm" src="~/AdminLTE-master/dist/img/duyuru-icon.png" alt="user image">

        <span class="username">

            <a href="#">@x.Category</a>

            <a href="#" class="float-right btn-tool"><i class="fas fa-times"></i></a>

        </span>

        <span class="description">@x.Date.ToString()</span>

    </div>

    <!-- /.user-block -->

    <p>

        @x.Contents

    </p>



</div>
 }